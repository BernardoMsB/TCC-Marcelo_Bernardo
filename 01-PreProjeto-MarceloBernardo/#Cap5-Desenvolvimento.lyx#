\begin_body

\begin_layout Chapter
Resultados e Discussões 
\begin_inset Index idx
status open

\begin_layout Plain Layout
Resultados e Discussões
\end_layout

\end_inset


\begin_inset CommandInset label
LatexCommand label
name "chap:Resultados_e_Discussoes"

\end_inset


\end_layout

\begin_layout Standard
\begin_inset ERT
status open

\begin_layout Plain Layout


